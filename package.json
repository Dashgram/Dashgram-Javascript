{
  "name": "@dashgram/javascript",
  "version": "1.0.0",
  "description": "Dashgram Analytics SDK for Telegram Mini Apps",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
