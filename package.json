  "repository": {
    "type": "git",
    "url": "https://github.com/dashgram/dashgram-javascript.git"
  },
  "bugs": {
    "url": "https://github.com/dashgram/dashgram-javascript/issues"
  },
  "homepage": "https://github.com/dashgram/dashgram-javascript#readme",
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
