{
  "name": "@dashgram/javascript",
  "version": "1.0.9",
  "description": "Dashgram Analytics SDK for Telegram Mini Apps",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "generate:version": "node scripts/generate-version.js",
    "build": "npm run generate:version && tsc",
    "dev": "npm run generate:version && tsc --watch",
    "build:umd": "rollup -c",
    "build:all": "npm run build && npm run build:umd",
    "prepublishOnly": "npm run build:all"
  },
  "keywords": [
    "dashgram",
    "analytics",
    "telegram",
    "mini-apps",
    "tracking",
    "telegram-mini-app",
    "webapp",
    "analytics-sdk"
  ],
  "author": "Dashgram",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dashgram/dashgram-javascript.git"
  },
  "bugs": {
    "url": "https://github.com/dashgram/dashgram-javascript/issues"
  },
  "homepage": "https://dashgram.io",
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.3.0",
    "rollup": "^2.79.2",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.3.3"
  }
}
